#include <stdio.h>

int main() {
    int arr[] = {12, 56, 34, 78, 100};
    int n = sizeof(arr) / sizeof(arr[0]);
    int max = arr[0];

    for (int i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }

    printf("Largest element of the array is: %d", max);

    return 0;
}
